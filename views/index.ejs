    <style>
         #Bann {
            height: 0;
          visibility: hidden;
          
         }
         #Bann1 {
            visibility: visible;
           width: 134%;
           box-shadow: rgba(14, 30, 37, 0.12) 0px 2px 4px 0px,
                                  rgba(14, 30, 37, 0.32) 0px 2px 16px 0px;
           background-size: cover;
           background-position: center;
          
         }
         #heb{
            visibility: hidden;
         }
       
        @media (max-width: 768px) {
         #Bann {
            visibility: hidden;
            box-shadow: rgba(14, 30, 37, 0.12) 0px 2px 4px 0px,
                                  rgba(14, 30, 37, 0.32) 0px 2px 16px 0px;
            height: 0;
           width: 100%;
           background-size: cover;
        background-position: top center;
        background-repeat: no-repeat;
         }
         #Bann1{
            width: 100%;
            height: 180px;
           background-size: contain;
           background-position: top center;     
        }
         #heb{
            visibility: hidden;
            margin-left: 10px;
         }
       
       }
 
       .grid-container {
         display: flex;
         flex-direction: row;
         flex-wrap: wrap;
         justify-content: center;
         align-items: center;
         align-content: center;
         padding: 0px 0px 0px 0px;
       }
 
       .grid-item {
         display: flex;
         flex-direction: column;
         flex-wrap: wrap;
         justify-content: center;
         align-items: center;
         margin: 30px;
       }
 
       .view-btn button {
         font-size: 17px;
         margin-top: 10px;
         height: 40px;
         width: 100px;
         background-color: rgb(29, 195, 29);
         border-radius: 10px;
         padding: 5px;
         color: white;
         border: 1px solid white;
       }
 
       .view-btn button:hover {
         background-color: white;
         border: 1px solid rgb(29, 195, 29);
         color: rgb(29, 195, 29);
       }
 
     </style>
