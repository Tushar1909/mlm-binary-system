    <!-- Humberger Begin -->
    <div class="humberger__menu__overlay"></div>
    <div class="humberger__menu__wrapper">
        <div class="humberger__menu__logo">
            <a href="/<%=id%>/index"><img src="/img/logo.png" alt=""></a>
        </div>
        <div class="humberger__menu__cart">
            <ul>
                <li><a href="/<%=id%>/shoping-cart"><i class="fa fa-shopping-bag"></i> <span><%= cartnum %></span></a></li>
            </ul>
            <% if(kyc != "Not verified"){ %>
                <div class="header__cart__price">wallet: <span><%= wallet %></span></div>
            <% } %> 
        </div>
        <div class="humberger__menu__widget">
            <div class="header__top__right__auth">
                <a href="/<%=id%>/profile"><i class="fa fa-user"></i>My Profile</a>
            </div>
        </div>
        <nav class="humberger__menu__nav mobile-menu">
            <ul>
                <li class="active"><a href="/<%=id%>/index">Home</a></li>
                <li><a href="/<%=id%>/shop-grid">Shop</a></li>
                <li><a href="/<%=id%>/shoping-cart">Cart</a></li>
                <li><a href="/<%=id%>/checkout">Check Out</a></li>
                <li><a href="/<%=id%>/contact">Contact</a></li>
            </ul>
        </nav>
        <div id="mobile-menu-wrap"></div>
        <div class="header__top__right__social">
            <a href="#"><i class="fa fa-facebook"></i></a>
            <a href="#"><i class="fa fa-twitter"></i></a>
            <a href="#"><i class="fa fa-linkedin"></i></a>
        </div>
        <div class="humberger__menu__contact">
            <ul>
                <li><i class="fa fa-envelope"></i> Homecarejpr@gmail.com</li>
            </ul>
        </div>
    </div>
    <!-- Humberger End -->

    <!-- Header Section Begin -->
    <header class="header">
        <div class="header__top">
            <div class="container">
                <div class="row">
                    <div class="col-lg-6">
                        <div class="header__top__left">
                            <ul>
                                <li><i class="fa fa-envelope"></i> Homecarejpr@gmail.com</li>
                                <li><i class="fa fa-phone"></i>+91 9950008527</li>
                            </ul>
                        </div>
                    </div>
                    <div class="col-lg-6">
                        <div class="header__top__right">
                            <div class="header__top__right__social">
                                <a href="#"><i class="fa fa-facebook"></i></a>
                                <a href="#"><i class="fa fa-twitter"></i></a>
                                <a href="#"><i class="fa fa-linkedin"></i></a>
                            </div>
                         
                            <div class="header__top__right__auth">
                                <a href="/<%=id%>/profile"><i class="fa fa-user"></i> My Profile</a>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="container">
            <div class="row">
                <div class="col-lg-3">
                    <div class="header__logo">
                        <a href="/<%=id%>/index"><img src="/img/logo.png" alt=""></a>
                    </div>
                </div>
                <div class="col-lg-6">
                    <nav class="header__menu">
                        <ul>
                            <li ><a href="/<%=id%>/index">Home</a></li>
                            <li class="active"><a href="/<%=id%>/shop-grid">Shop</a></li>
                            <li ><a href="/<%=id%>/shoping-cart">Cart</a></li>
                            <li ><a href="/<%=id%>/checkout">Check Out</a></li>
                            <li><a href="/<%=id%>/contact">Contact</a></li>
                        </ul>
                    </nav>
                </div>
                <div class="col-lg-3">
                    <div class="header__cart">
                        <ul>
                            <li><a href="/<%=id%>/shoping-cart"><i class="fa fa-shopping-bag"></i> <span><%= cartnum %></span></a></li>
                        </ul>
                        <% if(kyc != "Not verified"){ %>
                            <div class="header__cart__price">wallet: <span><%= wallet %></span></div>
                        <% } %> 

                    </div>
                </div>
            </div>
            <div class="humberger__open">
                <i class="fa fa-bars"></i>
            </div>
        </div>
    </header>
    <!-- Header Section End -->

    <!-- Hero Section Begin -->
   
    <!-- Hero Section End -->

    <!-- Breadcrumb Section Begin -->
    <section class="breadcrumb-section set-bg" data-setbg="/img/breadcrumb.jpg">
        <div class="container">
            <div class="row">
                <div class="col-lg-12 text-center">
                    <div class="breadcrumb__text">
                        <h2><%= productName %></h2>
                        <div class="breadcrumb__option">
                            <a href="/<%=id%>/index">Home</a>
                            <span> <%= productName %></span>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!-- Breadcrumb Section End -->

    <!-- Product Details Section Begin -->
    <section class="product-details spad">
        <div class="container">
            <div class="row">
                <div class="col-lg-6 col-md-6">
                    <div class="product__details__pic" >
                        <div style="background-color: #ded9d9d1; border-radius: 20px;">
                            <div class="product__details__pic__item" style="margin: 40px">
                                <img class="product__details__pic__item--large"
                                    src="<%=productImage%>" alt="Not">
                            </div>

                        </div>
                       
                   
                    </div>
                </div>
                <div class="col-lg-6 col-md-6">
                    <div class="product__details__text">
                        <h3><%= productName %></h3>
                        <div class="product__details__rating">
                            <i class="fa fa-star"></i>
                            <i class="fa fa-star"></i>
                            <i class="fa fa-star"></i>
                            <i class="fa fa-star"></i>
                            <i class="fa fa-star-half-o"></i>
                        </div>
                        <div class="product__details__price">Rs&nbsp;<%= productPrice %></div>
                        <p><%= productDescription %></p>
                        <form action="/shoping-cart/<%=id%>/<%= productName %>/<%= productPrice %>/add/" method="post">
                            <div class="product__details__quantity">
                                <div class="quantity">
                                    <div class="pro-qty">
                                        <input type="text" value="1" name="quantity">
                                    </div>
                                </div>
                            </div>
                            <% if(productquant != 0){ %>

                            <button type="submit" class="primary-btn">ADD TO CART</button>
                            <% } %> 

                        </form>
                        
                        <% if(productquant == 0){ %>

                        <ul>
                            <li><b>Availability :</b> <span style="color: red;">Out of Stock</span></li>
                          
                        </ul>
                        <% } %> 
                        <% if(productquant != 0){ %>
                            <ul>
                                <li><b>Availability :</b> <span>In Stock</span></li>
                              
                            </ul>
                            <% } %> 

                    </div>
                </div>
               
            </div>
        </div>
    </section>
    <div class="col-lg-12">
        <div class="product__details__tab">
            <ul class="nav nav-tabs" role="tablist">
                <li class="nav-item">
                    <a class="nav-link active" data-toggle="tab" href="#tabs-1" role="tab"
                        aria-selected="true">Description</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" data-toggle="tab" href="#tabs-2" role="tab"
                        aria-selected="false">Information</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" data-toggle="tab" href="#tabs-3" role="tab"
                        aria-selected="false">Reviews <span>(1)</span></a>
                </li>
            </ul>
            <div class="tab-content">
                <div class="tab-pane active" id="tabs-1" role="tabpanel">
                    <div class="product__details__tab__desc">
                        <h6>Products Description</h6>
                        <p><%= productDescription %></p>
                         
                    </div>
                </div>
                <div class="tab-pane" id="tabs-2" role="tabpanel">
                    <div class="product__details__tab__desc">
                        <h6>Products Infomation</h6>
                        <p><%= productDescription %></p>
                       
                    </div>
                </div>
                <div class="tab-pane" id="tabs-3" role="tabpanel">
                    <div class="product__details__tab__desc">
                        <h6>Product Review</h6>
                        <p><%= productDescription %></p>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- Product Details Section End -->



